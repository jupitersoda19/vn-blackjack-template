            "right": {
              "image": "/assets/characters/player.png",
              "name": "Player",
              "emotion": "nervous"
            },
            "left": {
              "image": "/assets/characters/victoria.png",
              "name": "Victoria",
              "emotion": "flirty"
            }
          },
          "speaker": "Player",
          "text": "Thanks, Victoria. It's nice to meet you."
